	<meta charset="utf-8">
	<title>Sistema Manager Project</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="description" content="Charisma, a fully featured, responsive, HTML5, Bootstrap admin template.">
	<meta name="author" content="Muhammad Usman">

	<!-- The styles -->
  <%= stylesheet_link_tag 'bootstrap-spacelab.css' %>
	<style type="text/css">
	  body {
		padding-bottom: 40px;
	  }
	  .sidebar-nav {
		padding: 9px 0;
	  }
	</style>
  <%= stylesheet_link_tag 'bootstrap-responsive.css' %>
  <%= stylesheet_link_tag 'charisma-app.css' %>
  <%= stylesheet_link_tag 'jquery-ui-1.8.21.custom.css' %>
  <%= stylesheet_link_tag 'fullcalendar.css' %>
  <%= stylesheet_link_tag 'chosen.css' %>
  <%= stylesheet_link_tag 'fullcalendar.print.css' %>
  <%= stylesheet_link_tag 'uniform.default.css' %>
  <%= stylesheet_link_tag 'colorbox.css' %>
  <%= stylesheet_link_tag 'jquery.cleditor.css' %>
  <%= stylesheet_link_tag 'jquery.noty.css' %>
  <%= stylesheet_link_tag 'jquery.cleditor.css' %>
  <%= stylesheet_link_tag 'noty_theme_default.css' %>
  <%= stylesheet_link_tag 'elfinder.min.css' %>
  <%= stylesheet_link_tag 'elfinder.theme.css' %>
  <%= stylesheet_link_tag 'jquery.iphone.toggle.css' %>
  <%= stylesheet_link_tag 'opa-icons.css' %>
  <%= stylesheet_link_tag 'uploadify.css' %>

    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
	<!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
	<!--[if lt IE 9]>
	  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->

	<!-- The fav icon -->
	<link rel="shortcut icon" href="img/favicon.ico">
		
</head>

<body>
		<!-- topbar starts -->
	<div class="navbar">
		<div class="navbar-inner">
			<div class="container-fluid">
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>
			<h2>Manager Project</h2>
               <div id="logo"></div> 
			
			
				
				<!-- theme selector starts -->
				
				<!-- theme selector ends -->
				
				<!-- user dropdown starts -->
				<div class="btn-group pull-right" >
					<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
						<i class="icon-user"></i><span class="hidden-phone"> admin</span>
						<span class="caret"></span>
					</a>
					<ul class="dropdown-menu">
						<li><a href="#">Profile</a></li>
						<li class="divider"></li>
						<li><a href="login.html">Logout</a></li>
					</ul>
				</div>
				<!-- user dropdown ends -->

			</div>
		</div>
	</div>
	<!-- topbar ends -->
		<div class="container-fluid">
		<div class="row-fluid">
				
			<!-- left menu starts -->
			<div class="span2 main-menu-span">
				<div class="well nav-collapse sidebar-nav">
					<ul class="nav nav-tabs nav-stacked main-menu">
						<li class="nav-header hidden-tablet">Main</li>
                      <li><%= link_to raw("<i class='icon-folder-open'></i><span class='hidden-tablet'> Proyectos</span>"), proyectos_path, :class=>'ajax-link' %></li>

					</ul>
					<label id="for-is-ajax" class="hidden-tablet" for="is-ajax"><input id="is-ajax" type="checkbox"> Ajax on menu</label>
				</div><!--/.well -->
			</div><!--/span-->
			<!-- left menu ends -->
			
			<noscript>
				<div class="alert alert-block span10">
					<h4 class="alert-heading">Warning!</h4>
					<p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
				</div>
			</noscript>
			
			<div id="content" class="span10">
			<!-- content starts -->
			

			<div>
				<ul class="breadcrumb">
					<li>
						<a href="#">Home</a> <span class="divider">/</span>
					</li>
					<li>
						<a href="#">Tables</a>
					</li>
				</ul>
			</div>
			
			<div class="row-fluid sortable">		
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2><i class="icon-user"></i> <%= t(@controlador_actual) %></h2>
						<div class="box-icon">
							<a href="#" class="btn btn-setting btn-round"><i class="icon-cog"></i></a>
							<a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
							<a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
						</div>
					</div>
					<div class="box-content">
						<%= yield%>
					</div>
				</div><!--/span-->
			
			</div><!--/row-->


			

			

    
					<!-- content ends -->
			</div><!--/#content.span10-->
				</div><!--/fluid-row-->
				
		<hr>

		<div class="modal hide fade" id="myModal">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">×</button>
				<h3>Settings</h3>
			</div>
			<div class="modal-body">
				<p>Here settings can be configured...</p>
			</div>
			<div class="modal-footer">
				<a href="#" class="btn" data-dismiss="modal">Close</a>
				<a href="#" class="btn btn-primary">Save changes</a>
			</div>
		</div>

		<footer>
			<p class="pull-left">Manager Project 2013 - <a href="mailto:info@gringsoft.com.ar">Información</a></p>
			<p class="pull-right">Powered by: <a href="http://www.gringsoftingenieria.com.ar">Gringsoft Ingeniería</a></p>
		</footer>
		
	</div><!--/.fluid-container-->

	<!-- external javascript
	================================================== -->
	<!-- Placed at the end of the document so the pages load faster -->

	<!-- jQuery -->
	<script src="assets/jquery-1.7.2.min.js"></script>
	<!-- jQuery UI -->
	<script src="assets/jquery-ui-1.8.21.custom.min.js"></script>
	<!-- transition / effect library -->
	<script src="assets/bootstrap-transition.js"></script>
	<!-- alert enhancer library -->
	<script src="assets/bootstrap-alert.js"></script>
	<!-- modal / dialog library -->
	<script src="assets/bootstrap-modal.js"></script>
	<!-- custom dropdown library -->
	<script src="assets/bootstrap-dropdown.js"></script>
	<!-- scrolspy library -->
	<script src="assets/bootstrap-scrollspy.js"></script>
	<!-- library for creating tabs -->
	<script src="assets/bootstrap-tab.js"></script>
	<!-- library for advanced tooltip -->
	<script src="assets/bootstrap-tooltip.js"></script>
	<!-- popover effect library -->
	<script src="assets/bootstrap-popover.js"></script>
	<!-- button enhancer library -->
	<script src="assets/bootstrap-button.js"></script>
	<!-- accordion library (optional, not used in demo) -->
	<script src="assets/bootstrap-collapse.js"></script>
	<!-- carousel slideshow library (optional, not used in demo) -->
	<script src="assets/bootstrap-carousel.js"></script>
	<!-- autocomplete library -->
	<script src="assets/bootstrap-typeahead.js"></script>
	<!-- tour library -->
	<script src="assets/bootstrap-tour.js"></script>
	<!-- library for cookie management -->
	<script src="assets/jquery.cookie.js"></script>
	<!-- calander plugin -->
	<script src='assets/fullcalendar.min.js'></script>
	<!-- data table plugin -->
	<script src='assets/jquery.dataTables.min.js'></script>

	<!-- chart libraries start -->
	<script src="assets/excanvas.js"></script>
	<script src="assets/jquery.flot.min.js"></script>
	<script src="assets/jquery.flot.pie.min.js"></script>
	<script src="assets/jquery.flot.stack.js"></script>
	<script src="assets/jquery.flot.resize.min.js"></script>
	<!-- chart libraries end -->

	<!-- select or dropdown enhancer -->
	<script src="assets/jquery.chosen.min.js"></script>
	<!-- checkbox, radio, and file input styler -->
	<script src="assets/jquery.uniform.min.js"></script>
	<!-- plugin for gallery image view -->
	<script src="assets/jquery.colorbox.min.js"></script>
	<!-- rich text editor library -->
	<script src="assets/jquery.cleditor.min.js"></script>
	<!-- notification plugin -->
	<script src="assets/jquery.noty.js"></script>
	<!-- file manager library -->
	<script src="assets/jquery.elfinder.min.js"></script>
	<!-- star rating plugin -->
	<script src="assets/jquery.raty.min.js"></script>
	<!-- for iOS style toggle switch -->
	<script src="assets/jquery.iphone.toggle.js"></script>
	<!-- autogrowing textarea plugin -->
	<script src="assets/jquery.autogrow-textarea.js"></script>
	<!-- multiple file upload plugin -->
	<script src="assets/jquery.uploadify-3.1.min.js"></script>
	<!-- history.js for cross-browser state change on ajax -->
	<script src="assets/jquery.history.js"></script>
	<!-- application script for Charisma demo -->
	<script src="assets/charisma.js"></script>
	
		
</body>
</html>
