<h1>Listing meta</h1>

<table>
  <tr>
    <th>Detalle</th>
    <th>Estado</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meta.each do |metum| %>
  <tr>
    <td><%= metum.detalle %></td>
    <td><%= metum.estado %></td>
    <td><%= link_to 'Show', metum %></td>
    <td><%= link_to 'Edit', edit_metum_path(metum) %></td>
    <td><%= link_to 'Destroy', metum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Metum', new_metum_path %>
