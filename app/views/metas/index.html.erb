<h1>Listing metas</h1>

<table>
  <tr>
    <th>Detalle</th>
    <th>Estado</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @metas.each do |meta| %>
  <tr>
    <td><%= meta.detalle %></td>
    <td><%= meta.estado %></td>
    <td><%= link_to 'Show', meta %></td>
    <td><%= link_to 'Edit', edit_meta_path(meta) %></td>
    <td><%= link_to 'Destroy', meta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meta', new_meta_path %>
